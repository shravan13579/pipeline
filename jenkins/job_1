pipeline {
  agent none
  stages {
     stage('BUILD') {
       agent any
        steps {
           sh '''
              #!/bin/bash
              pwd
              ls
              echo "this is build stage"
              '''
        }
     }
     stage('DEPLOY') {
       agent { label 'slave1' }
        steps {
           echo "this is deploy stage" 
        }
     }
     stage('TESTING1') {
       agent { label 'slave2' }
         steps {
           echo "this is testing stage"
         }  
     }
     stage('TESTING2') {
       agent { label 'built-in-node' }
         steps {
           echo "testing done"
         }
     }    
  }
}
